import java.util.Scanner;
class perfect{
public static void main(String args[])
{
Scanner input=new Scanner(System.in);
int n=input.nextInt();
int temp=n;
int sum=0;
for(int i=1;i<n;i++){
if(n%i==0)
{
sum=sum+i;
}
}
if(sum==temp)
{
System.out.print((n)+" is a perfect number");
}
else{
System.out.print((n)+"is not a perfect number");
}
}
}
